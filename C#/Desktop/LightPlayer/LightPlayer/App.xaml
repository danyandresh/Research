<Application x:Class="LightPlayer.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             StartupUri="Views/Folders.xaml"
             xmlns:lightPlayer="clr-namespace:LightPlayer"
             xmlns:converters="clr-namespace:LightPlayer.Converters"
             xmlns:system="clr-namespace:System;assembly=mscorlib">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Styles.xaml"/>
            </ResourceDictionary.MergedDictionaries>
            <lightPlayer:ViewModelLocator x:Key="Locator" />
            <converters:MediaElementStringParamsTuple x:Key="MediaElementStringParamsTuple"/>
            <converters:MediaElementIntParamsTuple x:Key="MediaElementIntParamsTuple"/>
            <converters:MediaInteractionConverter x:Key="MediaInteraction"/>
            <converters:TimeConverterFromSeconds x:Key="TimeConverterFromSeconds"/>
            <system:Boolean x:Key="True">True</system:Boolean>
            <system:Boolean x:Key="False">False</system:Boolean>
        </ResourceDictionary>
    </Application.Resources>
</Application>
