<Application x:Class="LightPlayer.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             StartupUri="Views/Folders.xaml"
             xmlns:lightPlayer="clr-namespace:LightPlayer"
             xmlns:converters="clr-namespace:LightPlayer.Converters">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Styles.xaml"/>
            </ResourceDictionary.MergedDictionaries>
            <lightPlayer:ViewModelLocator x:Key="Locator" />
            <converters:MultiValueParams x:Key="MultiValueParams"/>
            <converters:MediaInteractionConverter x:Key="MediaInteraction"/>
        </ResourceDictionary>
    </Application.Resources>
</Application>
